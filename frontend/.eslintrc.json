{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react/function-component-definition": 0,
    "react/prop-types": 0,
    "no-unused-vars": [1, { "argsIgnorePattern": "^_" }]
  }
}
